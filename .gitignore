backend/staticfiles/
backend/media/
*.sqlite3
*.db

# Env files (never commit secrets)
.env
.env.*
**/.env
**/.env.*
frontend/.env.local
frontend/.env.*.local
ops/docker/.env

# Cloudflare Wrangler local state
**/.wrangler/
.dev.vars
.dev.vars.*
wrangler.toml.local
wrangler.toml.*.local

# OS / Editor junk
.DS_Store
Thumbs.db
.idea/
.vscode/
*.swp
*.swo

